{
  "name": "@de-dale/cards",
  "version": "0.3.0-SNAPSHOT",
  "license": "ISC",
  "description": "Module de cartes pour le De-D'Ale",
  "repository": {
    "type": "git",
    "url": "git@github.com:de-dale/cards-on-shelves.git"
  },
  "scripts": {
    "build": "webpack --mode production",
    "deploy": "./scripts/deploy.sh",
    "eslint": "eslint src",
    "eslint-fix": "eslint src --fix",
    "release": "webpack --mode production && ./scripts/deploy.sh",
    "publish": "npm publish",
    "watch": "webpack --mode development --watch",
    "start": "webpack-dev-server --mode development --open",
    "test": "jest",
    "test-coverage": "jest --collectCoverage=true",
    "test-watch": "jest --watchAll"
  },
  "author": "fhiegel",
  "main": "index.js",
  "keywords": [
    "de-dale",
    "rpg",
    "cards",
    "tools"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "file-saver": "^1.3.3",
    "lodash": "^4.17.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "ajv": "^6.4.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.11",
    "csv-loader": "^2.1.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "jest": "^22.2.2",
    "jest-css-modules": "^1.1.0",
    "js-beautify": "^1.7.5",
    "prop-types": "latest",
    "react-test-renderer": "^16.2.0",
    "regenerator-runtime": "^0.11.1",
    "schema-utils": "^0.4.5",
    "style-loader": "^0.20.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^2.1.5",
    "webpack-dev-server": "^3.1.14",
    "xml-loader": "^1.2.1"
  },
  "jest": {
    "moduleNameMapper": {
      "^components(.*)$": "<rootDir>/src/app/components",
      "^utils(.*)$": "<rootDir>/src/app/core/utils$1",
      "^entities(.*)$": "<rootDir>/src/app/core/entities$1",
      "^store(.*)$": "<rootDir>/src/app/store$1"
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".css": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
